{
  "name": "caspian-seagull.github.io",
  "version": "1.0.0",
  "description": "[Caspian Seagull](http://caspian-seagull.github.io/index)",
  "main": "index.js",
  "scripts": {
    "pug": "pug source/pug/pages/**/*.pug -o ./ -P",
    "pug:watch": "npm run pug -- -w",
    "pug:build": "npm run pug -- -O '{Production: true}'",
    "js": "npm run js:bundle && npm run js:minify",
    "js:bundle": "concat-cli -f source/js/*.js -o js/bundle.js",
    "js:minify": "uglifyjs js/bundle.js -o js/bundle.min.js -c",
    "postcss": "postcss source/css/* -d css/ -x '.min.css' -c source/postcss.config.js",
    "server": "http-server ./ -o",
    "build": "npm run postcss & npm run js && npm run pug:build",
    "dev": "npm run pug:watch & npm run server",
    "fp": "git add --all && git commit -m \"fast push\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caspian-seagull/caspian-seagull.github.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caspian-seagull/caspian-seagull.github.io/issues"
  },
  "homepage": "https://github.com/caspian-seagull/caspian-seagull.github.io#readme",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "concat-cli": "^4.0.0",
    "http-server": "^0.9.0",
    "jstransformer-markdown-it": "^2.0.0",
    "markdown-it": "^8.3.1",
    "postcss-cli": "^3.2.0",
    "postcss-csso": "^2.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "uglify-js": "^2.8.22"
  }
}
